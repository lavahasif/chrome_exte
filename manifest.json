{
  "name": "chrom_exte",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "This is a flutter chrome extension",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/Icon-192.png"
  },
  "background": {
    "service_worker": "background-wrapper.js"
  },
  "permissions": [
    "tabs",
    "downloads",
    "bookmarks",
    "activeTab",
    "declarativeContent"
  ]
}
